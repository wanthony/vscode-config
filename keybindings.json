// Place your key bindings in this file to overwrite the defaults
[
    { "key": "ctrl+p", "command": "workbench.action.quickOpenNavigatePrevious",
                          "when": "inQuickOpen" },
    { "key": "ctrl+n", "command": "workbench.action.quickOpenNavigateNext",
                          "when": "inQuickOpen" },
    { "key": "alt-b",  "command": "cursorWordStartLeft",
                          "when": "editorTextFocus" },
    { "key": "alt-f",  "command": "cursorWordStartRight",
                          "when": "editorTextFocus" },
    { "key": "cmd+shift+[", "command": "workbench.action.previousEditor" },
    { "key": "cmd+shift+]", "command": "workbench.action.nextEditor" },
    { "key": "cmd+shift+j", "command": "workbench.action.terminal.focusPrevious",
                               "when": "terminalFocus" },
    { "key": "cmd+shift+k", "command": "workbench.action.terminal.focusNext",
                               "when": "terminalFocus" }
]